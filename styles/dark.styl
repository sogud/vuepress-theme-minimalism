.dark {
  background: #000000
  min-height: 100vh
  height: 100%
}

.dark>>>.header {
  background: #202020

  .container a {
    color: #afafaf
  }
}

.dark>>>.footer {
  background: #202020
}

.dark>>>.el-card {
  border: 1px solid #050505
  background-color: #202020
  color: #303133
  -webkit-transition: 0.3s
  transition: 0.3s
}

.dark>>>.el-card__header {
  border-bottom: 1px solid #343434
}